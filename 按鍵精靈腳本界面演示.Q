//<>------------------------------ 腳本子程序內容 --------------------------<>
EndScript
Sub OnScriptExit() '腳本停止事件
    Call Plugin.Media.Stop()
    MessageBox "測試結束，腳本停止！"
End Sub
Sub 人物狀態保護()
    //<>--------------------------人物補血設置
    If 血鍵 > 0 Then
        IfColor 5+(10-5)*補血,5,"0000FF",0 Then   '【坐標與顏色】需要自行修改
            //補血熱鍵
            KeyPress 血鍵, 1
            Delay 200
        End If
    End If
    //<>--------------------------人物補藍設置
    If 藍鍵 > 0 Then
        IfColor 5+(10-5)*補藍,10,"FF0000",0 Then  '【坐標與顏色】需要自行修改
            //補藍熱鍵
            KeyPress 藍鍵, 1
            Delay 200
        End If
    End If
    //<>--------------------------奶爸奶媽技能補血設置
    If 技能鍵 > 0 Then
        IfColor 5+(10-5)*補血,20,"0000FF",0 Then  '【坐標與顏色】需要自行修改
            //技能補血熱鍵
            KeyPress 技能鍵, 1
            Delay 1000  '技能釋放等待時間
        End If
    End If
End Sub




//<>------------------------------按鍵精靈腳本界面 - 簡稱：QUI --------------------------<>
//啟動腳本運行
Event Form1.Button6.Click
    RestartScript  '重啟腳本，用於從界面上點擊按鈕可以直接啟動腳本運行。
End Event

//重設 第一步設置 恢復初始狀態
Event Form1.Button2.Click
    Form1.遊戲路徑.Text="" '遊戲路徑
    Form1.遊戲帳號.Text="" '遊戲帳號
    Form1.遊戲密碼.Text="" '遊戲密碼
    Form1.網絡.ListIndex = 0   '選擇網絡
    Form1.服務器.ListIndex = 0 '服務器
    Form1.角色.ListIndex = 0   '角色選擇
End Event
//確定 第一步設置
Event Form1.Button1.Click
    Form1.Container1.Visible =False '設置界面
    Form1.Container2.Visible =True  '啟動界面
    Form1.狀態.Text = 1 '記錄界面顯示狀態
End Event
//返回 第一步設置
Event Form1.Button4.Click
    Form1.Container1.Visible =True  '設置界面
    Form1.Container2.Visible =False '啟動界面
    Form1.狀態.Text = 0 '記錄界面顯示狀態
End Event
//進入 更多設置
Event Form1.Button5.Click
    Form1.TabControl1.Tab = 1 '切換標籤頁
End Event
//返回 啟動腳本 界面
Event Form1.Button7.Click
    Form1.TabControl1.Tab = 0 '切換標籤頁
    Form1.Container1.Visible =False '設置界面
    Form1.Container2.Visible =True  '啟動界面
End Event

//讀取 界面設置狀態
Event Form1.LoadOver  '當窗體被加載完成以及ini配置內容都讀取完畢後觸發的事件
    Form1.TabControl1.Tab = 0 '切換標籤頁
    If Form1.狀態.Text = 0 then
        Form1.Container1.Visible = true  '設置界面
        Form1.Container2.Visible = false '啟動界面
    else
        Form1.Container1.Visible = false '設置界面
        Form1.Container2.Visible = true  '啟動界面
    End if
End Event

//添加可執行程序文件路徑
Event Form1.Button3.Click
    Dim filepath
    filepath = Plugin.File.SelectFile()  '打開文件選擇對話框
    If UCase(Right(filepath,4)) = ".EXE" Then  '檢查是不是選中 .exe 可執行文件
        Form1.遊戲路徑.Text = filepath
    Else
        If filepath <> "" Then 
            MsgBox "請選擇「.exe」可執行程序文件！"
        End If
    End If
End Event

//服務器初始化選擇
Event Form1.網絡.SelectChange
    Select Case Form1.網絡.ListIndex
    Case 0
        Form1.服務器.List = "桂林山水|鼓浪聞濤"  '【服務器名稱】需要自行修改
    Case 1
        Form1.服務器.List = "雲南大理|雲南麗江"  '【服務器名稱】需要自行修改
    Case 2
        Form1.服務器.List = "盤古開天|女媧造人"  '【服務器名稱】需要自行修改
    End Select
End Event

//<>------------------------------ 更多設置內容 --------------------------<>
//控制死亡處理下拉選項
Event Form1.死亡處理.SelectChange
    Dim 索引  
    索引 = Form1.死亡處理.ListIndex
    Select Case 索引
    Case 1
        Form1.選擇.Enabled = True
        Form1.路徑.Enabled = True
        Form1.播放.Enabled = True  
        Form1.停止.Enabled = True      
    Case Else
        Form1.選擇.Enabled = False
        Form1.路徑.Enabled = False
        Form1.播放.Enabled = False
        Form1.停止.Enabled = False
    End Select
End Event
//選擇MP3文件路徑
Event Form1.選擇.Click
    Dim MP3
    MP3 = Plugin.File.SelectFile()    '打開選擇文件對話框
    If UCase(Right(MP3,4)) = ".MP3" Or UCase(Right(MP3,4)) = ".WAV"  Then  '檢查是不是選中 .mp3 音樂文件
        Form1.路徑.Text = MP3
    Else
        If MP3 <> "" Then 
            MsgBox "請選擇「.mp3或者.wav」音樂格式的文件！"
        End If
    End If
End Event
//播放音樂測試
Event Form1.播放.Click
    Call Plugin.Media.Play(Form1.路徑.Text)
End Event
//停止播放音樂
Event Form1.停止.Click
    Call Plugin.Media.Stop()
End Event
//停止播放音樂
Event Form1.UnLoad  '當窗體被關閉卸載後觸發的事件  
    Call Plugin.Media.Stop()
End Event
//加載窗體時釋放MP3文件
Event Form1.Load  '當窗體被加載時觸發的事件   
    PutAttachment "C:\","*.wav"
End Event


